{
  "name": "smelte-template",
  "description": "Sapper template for Smelte",
  "version": "0.0.2",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "serve": "serve __sapper__/export",
    "cy:run": "cypress run",
    "lint": "prettier --write --plugin-search-dir=. ./src/**/*",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "now": "yarn build && now",
    "now-build": "sapper build --export"
  },
  "pre-commit": [
    "lint"
  ],
  "browserslist": "last 3 version",
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.18.1",
    "prismjs": "^1.29.0",
    "sirv": "^2.0.2",
    "smelte": "^1.1.8",
    "svelte": "^3.50.1",
    "svelte-image": "file:../",
    "svelte-waypoint": "^0.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.3",
    "@babel/runtime": "^7.19.0",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@now/node": "^1.8.5",
    "autoprefixer": "^10.4.12",
    "cssnano": "^5.1.13",
    "eslint": "^8.24.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.17",
    "postcss-custom-properties": "^12.1.9",
    "postcss-import": "^15.0.0",
    "postcss-nesting": "^10.2.0",
    "postcss-url": "^10.1.3",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.1",
    "rollup": "^2.79.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-includepaths": "^0.2.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.29.3",
    "sharp": "^0.31.1",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.8",
    "tailwindcss-elevation": "^1.0.1"
  }
}
